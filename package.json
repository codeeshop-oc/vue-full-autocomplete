{
  "name": "vue-full-autocomplete",
  "version": "1.0.2",
  "description": "Vue Full Autocomplete provides feature of autocomplete with GET and POST request and listing the results",
  "main": "dist/VueFullAutocomplete.ssr.js",
  "browser": "dist/VueFullAutocomplete.esm.js",
  "module": "dist/VueFullAutocomplete.esm.js",
  "unpkg": "dist/VueFullAutocomplete.min.js",
  "keywords": [
    "vue",
    "vue2",
    "autocomplete",
    "autocomplete-modal",
    "search",
    "full-autocomplete"
  ],
  "author": "Code-E-Shop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeeshop-oc/vue-full-autocomplete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeeshop-oc/vue-full-autocomplete.git"
  },
  "homepage": "https://codeeshop-oc.github.io/vue-full-autocomplete/",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "jest": {
    "verbose": true
  },
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "test": "jest --coverage",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "vue-fullpage-modal": "^0.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-numeric-separator": "^7.18.6",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.3.3",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "minimist": "^1.2.5",
    "rollup": "^2.52.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "vue": "^2.7.14",
    "vue-template-compiler": "^2.7.14"
  },
  "peerDependencies": {
    "vue": "^2.7.14"
  },
  "engines": {
    "node": ">=12"
  }
}
